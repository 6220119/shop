{
  "name": "react-prisma-graphql-shopping-cart-api",
  "scripts": {
    "start": "dotenv -- node dist/",
    "dev": "npm-run-all --parallel dev:*",
    "dev:api": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev:playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "deploy:local": "prisma deploy && graphql prepare",
    "deploy:dev": "prisma deploy -e .env.dev && graphql prepare && dotenv -e .env.dev now -e PRISMA_STAGE -e PRISMA_ENDPOINT -e PRISMA_CLUSTER -e PRISMA_SECRET -e APP_SECRET",
    "alias": "now alias"
  },
  "private": true,
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.4.3",
    "jsonwebtoken": "8.1.1",
    "now": "^10.1.0",
    "prisma-binding": "1.5.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.15.8",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "prisma": "1.3.2",
    "rimraf": "2.6.2",
    "ts-node": "5.0.0",
    "typescript": "2.7.2"
  },
  "now": {
    "alias": [
      "react-prisma-graphql-shopping-cart-api.kattcorp.co.uk",
      "react-prisma-graphql-shopping-cart-api.kattcorp.com"
    ]
  }
}
